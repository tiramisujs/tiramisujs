{
	"name": "@tiramisujs/microservices",
	"version": "0.1.0",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "tsup",
		"clean": "rm -rf dist && rm -rf .turbo",
		"dev": "tsup --watch"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			},
			"require": "./dist/index.js"
		}
	},
	"devDependencies": {
		"@types/ws": "^8.5.3",
		"tsup": "^6.1.3"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tiramisujs/tiramisujs.git"
	},
	"bugs": {
		"url": "https://github.com/tiramisujs/tiramisujs"
	},
	"dependencies": {
		"ws": "^8.11.0"
	}
}
